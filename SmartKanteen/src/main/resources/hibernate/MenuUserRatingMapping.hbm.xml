<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
 "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd"> 

<hibernate-mapping>
	<class name="com.mastek.topcoders.smartkanteen.bean.MenuUserRatingMapping" table="MENU_USER_RATING_MAPPING">
		<id name="menuUserRatingMappingId" type="java.lang.Integer">
			<column name="MENU_USER_RATING_MAPPING_ID" />
			<generator class="identity" />
		</id>
		<property name="rating" type="java.lang.Integer">
			<column name="RATING" />
		</property>
		<many-to-one name="itemId" column="ITEM_ID" unique="true"
			lazy="false" class="com.mastek.topcoders.smartkanteen.bean.Menu"
			not-null="true" cascade="all" />
		<many-to-one name="userId" column="user_id" unique="true"
			lazy="false" class="com.mastek.topcoders.smartkanteen.bean.User"
			not-null="true" cascade="all" />
 	</class>
</hibernate-mapping>

